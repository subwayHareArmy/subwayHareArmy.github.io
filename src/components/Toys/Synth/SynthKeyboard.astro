<style lang="scss">
  .synth-between-keys {
    fill: var(--synth-betweenkeys);
  }

  .synth-keys-shadow {
    fill: var(--synth-key-shadow);
  }

  .synth-keys-outline {
    fill: var(--synth-keys-outline);
  }

  .synth-key {
    cursor: pointer;
    transition: fill 0.4s ease-out;

    .pressed-overlay {
      fill: var(--synth-keys-outline);
      transform: translateY(2px);
      transition: transform 0.1s ease-out;
    }

    .crack {
      fill: var(--synth-keys-outline);
      opacity: 0;
      transition: opacity 0.4s ease-out;
    }

    .base {
      transition: fill 0.4s ease-out;
    }

    &.white .base {
      fill: var(--synth-whitekey);
    }

    &.black .base {
      fill: var(--synth-blackkey);
    }

    &.black .highlight {
      fill: var(--white-a4);
    }

    &.pressed {
      .base {
        transition: none;
        fill: var(--synth-whitekey-pressed);
      }

      &.black .base {
        fill: var(--synth-blackkey-pressed);
      }

      .pressed-overlay {
        transition: none;
        transform: translateY(0);
      }

      .crack {
        transition: none;
        opacity: 1;
      }
    }
  }

  .key-letter {
    fill: var(--black-a8);
    opacity: 0;
    transform: translateY(4px);
    transition: 0.4s ease-out;

    &.visible {
      opacity: 1;
      transform: translateY(0);
    }

    &.fadeout {
      opacity: 0;
      transition: opacity 1s ease-in;
      transition-delay: 0.2s;
    }
  }
</style>
